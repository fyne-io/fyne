<canvas padded size="150x200">
	<content>
		<widget pos="10,10" size="130x35" type="*widget.SelectEntry">
			<rectangle fillColor="inputBackground" pos="2,2" radius="4" size="126x31"/>
			<rectangle pos="1,1" radius="4" size="127x32" strokeColor="inputBorder" strokeWidth="2"/>
			<widget pos="0,2" size="94x31" type="*widget.Scroll">
